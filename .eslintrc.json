{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "import/no-unresolved": "off",
        "import/extensions": "off"
    },
    "env": {
        "jasmine": true
      }
}
